action (search) {
  description (search the world largest cartoon engine)
  type(Search)
  collect {
    input (keyword) {
      type (Keyword)
      min (Optional) max (One)
    }
    input (category) {
      type (Category)
      min (Optional) max (One)
    }
    input (author) {
      type (Author)
      min (Optional) max (One)
    }
  }
  output (Cartoon)
}
